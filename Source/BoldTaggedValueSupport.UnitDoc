      <Type Name="TDelphiFunctionType">
         <Summary></Summary>
         <Desc>This enumerated type is used for the various delphi functions types.
It is not really global, and will be moved to BoldMeta.
Values </Desc>
         <Enumeration>
            <EnumItem Name="dfNormal">
               <Desc>Normal method with no additional directives.</Desc>
            </EnumItem>
            <EnumItem Name="dfVirtual">
               <Desc>Method declared as virtual</Desc>
            </EnumItem>
            <EnumItem Name="dfOverride">
               <Desc>Overridden virtual or dynamic method.</Desc>
            </EnumItem>
            <EnumItem Name="dfDynamic">
               <Desc>Method declared as dynamic</Desc>
            </EnumItem>
            <EnumItem Name="dfAbstractVirtual">
               <Desc>Method declared as virtual; abstract;</Desc>
            </EnumItem>
         </Enumeration>
      </Type>
